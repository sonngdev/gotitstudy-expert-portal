{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-undef": "off",
        "no-unused-vars": "warn",
        "comma-dangle": ["warn", "always-multiline"],
        "react/jsx-filename-extension": "off",
        "react/prop-types": "warn",
        "import/prefer-default-export": "off"
    }
}
